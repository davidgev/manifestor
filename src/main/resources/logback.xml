<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{ISO8601} [%thread] %-5level [%logger{1}] - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="manifestor" level="INFO"/>

    <root>
        <priority value="INFO"/>
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>
